    <link rel="apple-touch-icon" sizes="120x120" href="<%= static_path(@conn, "/img/fav-icons/apple-icon-120x120.png") %>">
    <link rel="apple-touch-icon" sizes="144x144" href="<%= static_path(@conn, "/img/fav-icons/apple-icon-144x144.png") %>">
