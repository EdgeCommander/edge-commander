<body class="m-page--fluid m--skin- m-content--skin-light2 m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-dark m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default" onload="load_page('<%= @conn.request_path %>')" id="body_pannel">
    <!-- begin:: Page -->
        <div class="m-grid m-grid--hor m-grid--root m-page" id="page_content_main">
            <!-- BEGIN: Header -->
            <%= render_existing @view_module, "header.html", assigns %>
                <!-- END: Header -->
            <!-- begin::Body -->
            <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-body">
                <!-- BEGIN: Left Aside -->
                <%= render_existing @view_module, "menu_list.html", assigns %>
                <!-- END: Left Aside -->
                <div class="m-grid__item m-grid__item--fluid m-wrapper content_right_panel" id="remote_data">
                    <div id="sims_main"></div>
                    <div id="nvr_main"></div>
                    <div id="status_rpt_main"></div>
                    <div id="router_main"></div>
                    <div id="sites_main"></div>
                    <div id="commands_main"></div>
                    <div id="sms_main"></div>
                    <div id="api_main"></div>
                    <div id="profile_main"></div>
                    <div id="sims_details_main"></div>
                </div>
                <div class="action_btn" id="action_btn"></div>
            </div>
        </div>
            <!-- end:: Body -->
        <!-- begin::Scroll Top -->
        <div class="m-scroll-top m-scroll-top--skin-top" data-toggle="m-scroll-top" data-scroll-offset="500" data-scroll-speed="300">
            <i class="la la-arrow-up"></i>
        </div>
        <!-- end::Scroll Top -->
    </div>
    <!-- end:: Page -->
</body>